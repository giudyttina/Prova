    <ab>
                        <lb n="1"/>
                        
                        
                            
                            <persName type="divine"><name nymRef="#Iuppiter"><expan><abbr>I</abbr><ex>ovi</ex></expan></name> <addname type="epithet" nymRef="#optimus"><expan><abbr>o</abbr><ex>ptimo</ex></expan></addname> <addname type="epithet" nymRef="#Maximus"><expan><abbr>m</abbr><ex>aximo</ex></expan></addname></persName>
        <lb n="2"/><persName type="attested"><name type="nomen" nymRef="#Publius"><expan><abbr>P</abbr><ex>ublius</ex></expan></name> <name type="gentilicium" nymRef="#Aelius">Aelius</name> <lb n="3"/><name type="cognomen" nymRef="#Mucianus">Mucia <lb n="4"/>nus</name></persName>
        <lb n="5"/><expan><abbr>v</abbr><ex>otum</ex></expan> <expan><abbr>s</abbr><ex>olvit</ex></expan> <expan><abbr>l</abbr><ex>ibens</ex></expan> <expan><abbr>m</abbr><ex>erito</ex></expan>.
                        
                        
